[database]
type = postgres
host = 127.0.0.1:5432
name = {{grafana_db_name}}
user = {{grafana_db_user}}
password = {{grafana_db_pass}}

[server]
protocol = http
http_addr = 127.0.0.1
http_port = 3000

[security]
admin_user = {{grafana_admin_user}}
admin_password = {{grafana_admin_pass}}
secret_key = {{grafana_secret_key}}

[users]
allow_sign_up = false
